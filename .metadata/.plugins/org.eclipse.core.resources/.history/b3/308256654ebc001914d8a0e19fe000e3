package app;

public class test1 {
	

	public static void main(String[] args) 
	{
		int p[]={0,1,5,8,9,10,17,20,24,30};
		int q=-1;
		int n=9;
		int r[]=new int[n+1];
		for (int i=0;i<r.length; i++) 
		{
			r[i]=0;
		}
		for (int i=1;i<=n; i++)
		{
			for(int j=1;j<=i;j++)
			{				
				if(q<p[j]+r[i-j])
				{
					q=p[j]+r[i-j];
				}
			}
			r[i]=q;
		}
		System.out.print(r[5]);
	}

}
